{
  "index": {
    "prefix": "index",
    "description": "create a skeleton for index files vitepress",
    "body": [
      "---",
      "title: ${1:${TM_DIRECTORY/.*[\\\/]([^\\\/]+)/$1/}}",
      "prev: false",
      "next: false",
      "lastUpdated: false",
      "---",
      "",
      "# ${1:${TM_DIRECTORY/.*[\\\/]([^\\\/]+)/$1/}}",
      "",
      "- [${2:title}](${3:link.md})",
      ""
    ]
  },
  "New Code Section": {
    "prefix": "nsc",
    "body": ["", "---", "", "### $1:", "", "```$2", "$3", "", "```", ""],
    "description": "New section with code block"
  },
  "Fenced Code Block": {
    "prefix": "code",
    "body": ["```$1", "$2", "```", "$0"],
    "description": "New fenced code block"
  }
}
